    <validationRules>
        <fullName>LID__cannot_edit_Priority_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <description>Validation Rule not in use</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>Priority</errorDisplayField>
        <errorMessage>You cannot edit the Priority field on LInkedin Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_assigned_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <errorConditionFormula>OR(RecordType.Name = &quot;inMail&quot;, RecordType.Name = &quot;Messages&quot;, RecordType.Name = &quot;PointDrive Presentation Created&quot;, RecordType.Name = &quot;PointDrive Presentation Viewed&quot;, RecordType.Name = &quot;LinkedIn_Call&quot;)
&amp;&amp; (ISCHANGED( OwnerId ))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You cannot edit the Assigned To field on LinkedIn Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_status_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <errorConditionFormula>OR(RecordType.Name = &quot;inMail&quot;, RecordType.Name = &quot;Messages&quot;, RecordType.Name = &quot;PointDrive Presentation Created&quot;, RecordType.Name = &quot;PointDrive Presentation Viewed&quot;, RecordType.Name = &quot;LinkedIn_Call&quot;)
&amp;&amp; (ISCHANGED( Status ))</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>You cannot edit the Assigned To field on LinkedIn Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Related_To_required_for_Contacts</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  
OR(
TEXT( Type ) = &quot;Intro Call&quot;,
TEXT( Type ) = &quot;Discovery Call&quot;,
TEXT( Type ) = &quot;Demo&quot;),
ISBLANK(WhatId),BEGINS( WhoId , &quot;003&quot;))</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>Please enter &apos;Related To&apos; in order to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manadate_PhysicalGift_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL( Gift_Type__c , &apos;Physical Gift&apos;) ,
OR
(
ISBLANK( Contact_Mailing_Address__c ) ,
ISBLANK( Gift_URL__c )
)
)</errorConditionFormula>
        <errorDisplayField>Contact_Mailing_Address__c</errorDisplayField>
        <errorMessage>&apos;Contact Mailing Address&apos;, and &apos;Gift URL&apos; are mandatory when Gift Type is &apos;Physical Gift&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Comments</fullName>
        <active>true</active>
        <description>Mandate Comments for INtro Call/Discovery Call/Demo Call.</description>
        <errorConditionFormula>AND ( Share_Meeting_Notes__c = True, LEN( Description ) = 0)</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>Please enter Comments to share meeting notes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Date_DC_Held</fullName>
        <active>false</active>
        <description>&apos;Date DC Held&apos;  is required when Task Status is &apos;Complete&apos;</description>
        <errorConditionFormula>AND(
ISPICKVAL( Status , &apos;Complete&apos;),  
(ISBLANK( Date_DC_Held__c ))
)</errorConditionFormula>
        <errorMessage>Please enter &apos;Date DC Held&apos;  to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Date_Demo_Held</fullName>
        <active>false</active>
        <description>&apos;Date Demo Held&apos; is required when Task Status is &apos;Complete&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL( Type , &apos;Demo&apos;), 
ISPICKVAL( Status , &apos;Complete&apos;),  
ISBLANK(TEXT( Date_Demo_Held__c ))  
)</errorConditionFormula>
        <errorMessage>Please enter &apos;Date Demo Held&apos; to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Meeting_Type</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Type) = &quot;Meeting&quot; &amp;&amp;   ISBLANK(TEXT( Meeting_Type__c))</errorConditionFormula>
        <errorDisplayField>Meeting_Type__c</errorDisplayField>
        <errorMessage>Meeting Type is required when Type is &quot;Meeting&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Next_Step</fullName>
        <active>true</active>
        <description>Next Step is required when the Status is Completed or Canceled.</description>
        <errorConditionFormula>OR(
AND(
ISPICKVAL(Status, &apos;Cancel&apos;),    ISBLANK(TEXT(Next_Step__c ) )
),
AND(
ISPICKVAL(Status, &apos;Complete&apos;),    ISBLANK(TEXT(Next_Step__c ) )
)
)</errorConditionFormula>
        <errorMessage>Please enter &apos;Next Step&apos; to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Other_Reason</fullName>
        <active>true</active>
        <description>&apos;Other Reason&apos; is required when the &apos;Back to the Marketing&apos;. is &apos;Other&apos;</description>
        <errorConditionFormula>AND(ISPICKVAL( Reason__c, &apos;Other&apos; ),  ISBLANK(Other_Reason__c))</errorConditionFormula>
        <errorMessage>Please enter &apos;Other Reason&apos; to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Reason</fullName>
        <active>true</active>
        <description>Reason is required when the &apos;Next Step&apos; is &apos;Back to the Marketing&apos;.</description>
        <errorConditionFormula>AND(ISPICKVAL(  Next_Step__c , &apos;Back to the Marketing&apos;) ,  ISBLANK(TEXT( Reason__c ))
)</errorConditionFormula>
        <errorMessage>Please enter &apos;Reason&apos; to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Meeting_Notes</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Type) = &quot;Meeting&quot;   &amp;&amp;   (LEN( Meeting_Recap__c ) &lt;= 0) &amp;&amp;  ((TEXT( Status ) = &quot;Complete&quot; )  ||   (TEXT( Status ) = &quot;Completed&quot; ))</errorConditionFormula>
        <errorDisplayField>Meeting_Recap__c</errorDisplayField>
        <errorMessage>Please enter &quot;Meeting Recap&quot; in order to complete the Task.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>activities__CloseTasks</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Tasks</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)}
var left = (screen.width/2)-(320/2);
var top = (screen.height/2)-(200/2);
var records = {!GETRECORDIDS($ObjectType.Task)};
var strIDs=&apos;&apos;;
if (records[0] == null)
{
alert(&apos;Please select atleast one record&apos;);
}
else
{
for (var n=0; n&lt;records.length; n++)
{
if(strIDs == &apos;&apos;)
strIDs = records[n];
else
strIDs = strIDs + &quot;,&quot; + records[n];
}
window.location.href = &apos;/apex/activities__CloseTasks?strIDs=&apos;+strIDs+&apos;&amp;retURL=&apos;+ window.location.href;
}</url>
    </webLinks>
    <webLinks>
        <fullName>activities__Mass_Update</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mass Update Tasks</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
var records =  {!GETRECORDIDS( $ObjectType.Task)};
var returnURL = window.location.href;
if(records != null &amp;&amp; records != &apos;&apos;){
     mywin = window.open(&apos;/apex/activities__UpdateTasks?Ids=&apos;+records+&apos;&amp;retURL=&apos;+returnURL , &apos;_top&apos;,
                                             &apos;height=400,width=600,status=yes,toolbar=no,menubar=no,location=no,center=yes,resizable=no&apos;);
     mywin.focus();
}else{
    alert(&apos;Please select atleast one task.&apos;);
}</url>
    </webLinks>
